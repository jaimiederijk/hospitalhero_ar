    <section id="dancecontainer">
      <button type="button" name="button" id="dancepause" class="hidden">Pause</button>
      <h3 class="eatdance"><em id=dancepoints>0</em></h3>
      <div id="dancelightscontainer">
        <img id="avatar" src="images/avatar.svg" alt="">
        <img id="aardvarkdodo" src="images/object-aardvark.png" alt="">
        <div id="dancelights">
          <div class="lamp colorA"><img src="images/lamp.svg" alt="lamp">
            <div class="beamoflight">

            </div>
          </div>
          <div class="lamp colorB"><img src="images/lamp.svg" alt="lamp">
            <div class="beamoflight">

            </div>
          </div>
          <div class="lamp colorB"><img src="images/lamp.svg" alt="lamp">
            <div class="beamoflight">

            </div>
          </div>
          <div class="lamp colorD"><img src="images/lamp.svg" alt="lamp">
            <div class="beamoflight">

            </div>
          </div>
          <div class="lamp colorB"><img src="images/lamp.svg" alt="lamp">
            <div class="beamoflight">

            </div>
          </div>

        </div>
      </div>


      <div id="dancebuttonscontainer">

        <div id="danceinstructor">
          <em class="movedone colorA">A</em>
          <em class="movedone colorD">D</em>
          <em class="movedone colorD">D</em>
          <em class="movedone colorB">B</em>
          <em class="movedone colorB currentdancemove">B</em>
          <em class="movedone colorD">D</em>
          <em class="movedone colorD">D</em>


        </div>
        <div id="danceinstructorhighlight">
          <em class="colorB">B</em>
          <em class="colorB">B</em>
          <em class="colorD">D</em>
        </div>
        <button type="button" name="button" class="dancebuttons">A</button>
        <button type="button" name="button" class="dancebuttons">B</button>
        <button type="button" name="button" class="dancebuttons">C</button>
        <button type="button" name="button" class="dancebuttons">D</button>
      </div>

    </section>
  </body>
